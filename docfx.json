{
  "metadata": [
    {
      "src": [{ "files": ["src/artifacts/bin/BD.SteamClient8/**/BD.SteamClient8.dll"]}],
      "dest": "api/BD.SteamClient8"
    },
    {
      "src": [{ "files": ["src/artifacts/bin/BD.SteamClient8.Impl/**/BD.SteamClient8.Impl.dll"]}],
      "dest": "api/BD.SteamClient8.Impl"
    },
    {
      "src": [{ "files": ["src/artifacts/bin/BD.SteamClient8.Models/**/BD.SteamClient8.Models.dll"]}],
      "dest": "api/BD.SteamClient8.Models"
    },
    {
      "src": [{ "files": ["src/artifacts/bin/BD.SteamClient8.Primitives/**/BD.SteamClient8.Primitives.dll"]}],
      "dest": "api/BD.SteamClient8.Primitives"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "toc.yml",
          "*.html",
          "*.md",
          "**/**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "ref/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "res/DocFXIcon.png",
          "README.md"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/default"
    ],
    "globalMetadata": {
      "_appName": "SteamClient 文档",
      "_appTitle": "SteamClient",
      "_appLogoPath": "res/DocFXIcon.png",
      "_appFooter": "©️ 江苏蒸汽凡星科技有限公司. All rights reserved.",
      "_enableSearch": false
    },
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}