<Project>
	<Import Project="$(MSBuildThisFileDirectory)Version.props" />
	<Import Project="$(MSBuildThisFileDirectory)Path.Submodule.props" />

	<!-- üëá ÂÖ®Â±Ä Property ÈÖçÁΩÆ -->
	<PropertyGroup>
		<Deterministic>true</Deterministic>
		<LangVersion>preview</LangVersion>
		<Nullable>enable</Nullable>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
		<UseArtifactsOutput>true</UseArtifactsOutput>
		<SuppressTfmSupportBuildWarnings>true</SuppressTfmSupportBuildWarnings>
		<NoWarn>$(NoWarn);NU1504;NU1507;MSB4011;NU5048;NU5104;RS1041</NoWarn>
		<GenerateAssemblyInfo>true</GenerateAssemblyInfo>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NeutralLanguage>zh-Hans</NeutralLanguage>
		<!-- üëá Ê∫êÁîüÊàêÂô®ÁîüÊàêÁöÑ C# ‰∏¥Êó∂Ê∫êÁ†ÅÊñá‰ª∂Âú®ÁîüÊàêÁªìÊùüÂêéÊòØÂê¶‰∏çÂà†Èô§Êñá‰ª∂ÔºåÁî®‰∫éÁîüÊàêÈîôËØØ‰ª£Á†ÅÊó∂Êü•Êñá‰ª∂ÂÜÖÂÆπÁ∫†Ê≠£ÁîüÊàêÂô®ÔºåÂêØÁî®Êó∂‰ºöÂØºËá¥ VS ÁöÑ Android ÁÉ≠ÈáçËΩΩÂú®ÁîüÊàêÊó∂Âá∫Èîô -->
		<!--<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>-->
	</PropertyGroup>

	<!-- üëá TFM != .NET Framework 3.5 -->
	<PropertyGroup Condition="!$(TargetFramework.StartsWith('net35'))">
		<ImplicitUsings>true</ImplicitUsings>
	</PropertyGroup>

	<!-- üëá Á®ãÂ∫èÈõÜ‰ø°ÊÅØ -->
	<PropertyGroup>
		<!--8.yy.1MMdd.1hhmm-->
		<Version>8.25.10618.11900</Version>
		<Copyright>¬©Ô∏è $(Company). All rights reserved.</Copyright>
		<Authors>$(Company)</Authors>
		<FileVersion>$(Version)</FileVersion>
		<AssemblyVersion>$(Version)</AssemblyVersion>
	</PropertyGroup>

	<!-- üëá CompilerVisibleProperty ÈÖçÁΩÆÊ∫êÁîüÊàêÂô®‰∏≠ MSBuild Property ÂèØËßÅÊÄß -->
	<ItemGroup>
		<CompilerVisibleProperty Include="Deterministic" />
	</ItemGroup>

	<!-- üëá InternalsVisibleToAttribute -->
	<ItemGroup>
		<Compile Condition="$(MSBuildProjectName) != 'BD.SteamClient8.UnitTest' AND $(IsTestProject) != 'true'" Include="$(MSBuildThisFileDirectory)InternalsVisibleTo.BD.SteamClient8.UnitTest.cs">
			<LinkBase>Properties\InternalsVisibleTo</LinkBase>
		</Compile>
	</ItemGroup>
</Project>
