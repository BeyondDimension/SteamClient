<Project>
	<!-- 全局 using | Global usings -->
	<!-- 平台 != ios,android -->
	<ImportGroup Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) != 'ios' AND $([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) != 'android'">
		<Import Project="$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.ValveKeyValue.props" />
		<Import Project="$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.SteamAchievementManager.props" />
	</ImportGroup>
</Project>