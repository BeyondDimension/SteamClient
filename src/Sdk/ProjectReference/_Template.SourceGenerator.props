<Project>
	<!-- 项目引用（源生成器） -->
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.$(MSBuildThisFileName).props') AND $(MSBuildProjectName) == $(MSBuildThisFileName)" Project="$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.$(MSBuildThisFileName).props" />
	<ItemGroup Condition="Exists('$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\$(MSBuildThisFileName).csproj') AND $(MSBuildProjectName) != $(MSBuildThisFileName)">
		<ProjectReference Include="$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\$(MSBuildThisFileName).csproj">
			<OutputItemType>Analyzer</OutputItemType>
			<ReferenceOutputAssembly>false</ReferenceOutputAssembly>
		</ProjectReference>
	</ItemGroup>
</Project>